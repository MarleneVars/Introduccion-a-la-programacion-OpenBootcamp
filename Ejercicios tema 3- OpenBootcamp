//Actividad 1
//Función con tres parámetros
public static int suma(int num1, int num2, int num3) {
    return num1 + num2 + num3;
}

//Llamar a la función en el main y darle valores
public static void main(String[] args) {
    int resultado = suma(2, 3, 5);
    System.out.println(resultado); 
}

//Actividad 2
//Clase coche
public class Coche {
    private int numPuertas;
    
    //Constructor
    public Coche(int numPuertas) {
        this.numPuertas = numPuertas;
    }
    
    //Función que incrementa el numero de puertas
    public void aumentarPuertas(){
        this.numPuertas++;
    }
    
    //Getter
    public int getNumPuertas(){
        return this.numPuertas;
    }
}

//Crear objeto miCoche
Coche miCoche = new Coche(4);

//Añadir una puerta
miCoche.aumentarPuertas();

//Mostrar el numero de puertas
System.out.println(miCoche.getNumPuertas());
